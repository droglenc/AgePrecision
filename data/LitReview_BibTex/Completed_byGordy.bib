	year = {2018},
	keywords = {growth curve, marginal increment analysis, otolith mass, Protandry, sexual pattern, VGBF},
	pages = {382--390},
	file = {Full Text PDF:/Users/gordy/Zotero/storage/72ZAVI3E/Law and Mitcheson - 2018 - Age and growth of black seabream Acanthopagrus sch.pdf:application/pdf}
}

