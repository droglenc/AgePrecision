	note = {tex.copyright: © 2020 Wiley-VCH GmbH},
	keywords = {otoliths, population dynamics, spawning potential ratio},
	file = {Full Text PDF:/Users/gordy/Zotero/storage/ELH7CPY5/Sammons - Age, growth, and fishery assessment of spotted san.pdf:application/pdf}
}

