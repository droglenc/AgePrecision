	pages = {177--195},
	file = {Jefferson et al. - 2019 - Age and growth of gray triggerfish ( Balistes c.pdf:/Users/gordy/Zotero/storage/8548FLZI/Jefferson et al. - 2019 - Age and growth of gray triggerfish ( Balistes c.pdf:application/pdf}
}

@article{jurado-ruzafa_age_2018,
